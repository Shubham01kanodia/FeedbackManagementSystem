	<bean id="Index_Names" class="com.Project.FeedbackManagementSystem.ConfHandler.ElasticConfiguration">
		<property name = "student_basic_details">
			<bean id = "Document_Name" class = "com.Project.FeedbackManagementSystem.ConfHandler.ElasticColumnMapping">
				<constructor-arg name = "indexName" value="student_basic_details" />
				<constructor-arg name = "documentName" value="student_basic_details_doc" />
				<constructor-arg name = "primaryField" value="studentId" />
				<constructor-arg name="columnsList" >
					<map>
						<entry key="studentId" value="String" />
						<entry key="studentCollege" value="String" />
						<entry key="studentFirstName" value="String" />
						<entry key="studentLastName" value="String" />
						<entry key="studentMiddleName" value="String" />
						<entry key="studentEmailId" value="String" />
						<entry key="studentMobileNumber1" value="Long" />
						<entry key="studentMobileNumber2" value="Long" />
						<entry key="studentAddressState" value="String" />
						<entry key="studentAddressCity" value="String" />						
						<entry key="studentAddressLocation" value="String" />
						<entry key="studentPincode" value="Integer" />
						<entry key="studentCountry" value="String" />
						<entry key="studentCourse" value="String" />
						<entry key="studentBranch" value="String" />
						<entry key="studentSemester" value="String" />  
					</map>
				</constructor-arg>
			</bean>
		</property>
	</bean>

</beans>